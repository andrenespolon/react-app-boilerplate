{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
      "airbnb",
      "prettier",
      "prettier/react"
  ],
  "globals": {
    "Atomics": "readonly",
    "ShareArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "plugins": [
      "react",
      "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": [
      "warn", 
      { "extensions": [".jsx", ".js"] }
    ],
    "import/prefer-default-export": "off",
    "no-param-reassign": "off",
    "no-console": ["error"],
    "react/prop-types": "off"
  }
}
